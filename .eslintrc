{
  "globals": {
    "__dirname": "readonly",
    "process": "readonly",
    "__STORE": "writable"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [
      "warn",
      "always"
    ],
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false
      }
    ],
    "comma-dangle": "off",
    "curly": "off",
    "eqeqeq": "off",
    "indent": "off",
    "linebreak-style": "off",
    "no-tabs": "off",
    "no-var": "off",
    "quotes": "off",
    "strict": "off",
    "react/jsx-indent": "off",
    "react/jsx-closing-bracket-location": "off",
    "react/jsx-closing-tag-location": "off",
    "react/jsx-indent-props": "off",
    "react/jsx-space-before-closing": "off",
    "react/style-prop-object:": "off",
  }
}
